{
  "expo": {
    "name": "공주대처럼",
    "slug": "like-knu",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "31",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF",
        "dark": {
          "image": "./assets/splash-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#101012"
        }
      },
      "bundleIdentifier": "com.woopaca.likeknu",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsArbitraryLoadsInWebContent": true
        },
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDevelopmentRegion": "ko",
        "CFBundleLocalizations": [
          "ko"
        ]
      },
      "appStoreUrl": "https://apps.apple.com/kr/app/%EA%B3%B5%EC%A3%BC%EB%8C%80%EC%B2%98%EB%9F%BC/id6499512208"
    },
    "android": {
      "versionCode": 31,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF",
        "dark": {
          "image": "./assets/splash-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#101012"
        }
      },
      "package": "ac.knu.likeknu",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=ac.knu.likeknu"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ],
    "scheme": "like-knu",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e7aa67eb-52b1-48f3-85b7-9faee242f8e8"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e7aa67eb-52b1-48f3-85b7-9faee242f8e8"
    }
  }
}
